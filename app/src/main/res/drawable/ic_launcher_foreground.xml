<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Centered, perfectly minimalist clock -->
    <group
        android:translateX="54"
        android:translateY="54">
        
        <!-- Main clock circle -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M0,0m-28,0a28,28 0,1 1,56 0a28,28 0,1 1,-56 0" />
        
        <!-- Clock hands - showing 10:10 (classic time) -->
        <!-- Hour hand -->
        <path
            android:fillColor="#2C5AA0"
            android:pathData="M-1,-2h2v-12h-2z"
            android:fillAlpha="0.9" />
        
        <!-- Minute hand -->
        <path
            android:fillColor="#2C5AA0"
            android:pathData="M-0.5,-2h1v-16h-1z"
            android:fillAlpha="0.9" />
        
        <!-- Center dot -->
        <path
            android:fillColor="#3DDC84"
            android:pathData="M0,0m-2,0a2,2 0,1 1,4 0a2,2 0,1 1,-4 0" />
        
        <!-- Single floating element for "floating" concept -->
        <path
            android:fillColor="#3DDC84"
            android:pathData="M20,-12m-4,0a4,4 0,1 1,8 0a4,4 0,1 1,-8 0"
            android:fillAlpha="0.7" />
            
    </group>
</vector>
