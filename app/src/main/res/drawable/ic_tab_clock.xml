<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24"
    android:tint="?attr/colorOnSurface">
    
    <!-- Clock tab icon - matches app theme -->
    <group
        android:translateX="12"
        android:translateY="12">
        
        <!-- Clock circle -->
        <path
            android:fillColor="@android:color/black"
            android:pathData="M0,0m-10,0a10,10 0,1 1,20 0a10,10 0,1 1,-20 0
                           M0,0m-8,0a8,8 0,1 0,16 0a8,8 0,1 0,-16 0" />
        
        <!-- Clock hands showing 10:10 -->
        <!-- Hour hand -->
        <path
            android:fillColor="@android:color/black"
            android:pathData="M-1,-1h2v-4h-2z" />
        
        <!-- Minute hand -->
        <path
            android:fillColor="@android:color/black"
            android:pathData="M-0.5,-1h1v-6h-1z" />
        
        <!-- Center dot -->
        <path
            android:fillColor="@android:color/black"
            android:pathData="M0,0m-1.5,0a1.5,1.5 0,1 1,3 0a1.5,1.5 0,1 1,-3 0" />
            
    </group>
</vector>