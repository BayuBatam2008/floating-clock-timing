<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24"
    android:tint="?attr/colorOnPrimary">
    
    <!-- Optimized notification icon - simplified clock design -->
    <group
        android:translateX="12"
        android:translateY="12">
        
        <!-- Main clock circle - outline only for better visibility at small size -->
        <path
            android:fillColor="@android:color/white"
            android:pathData="M0,0m-9,0a9,9 0,1 1,18 0a9,9 0,1 1,-18 0
                           M0,0m-7,0a7,7 0,1 0,14 0a7,7 0,1 0,-14 0" />
        
        <!-- Clock hands - thicker for notification size -->
        <!-- Hour hand -->
        <path
            android:fillColor="@android:color/white"
            android:pathData="M-1,-1h2v-4h-2z" />
        
        <!-- Minute hand -->
        <path
            android:fillColor="@android:color/white"
            android:pathData="M-0.5,-1h1v-5.5h-1z" />
        
        <!-- Center dot -->
        <path
            android:fillColor="@android:color/white"
            android:pathData="M0,0m-1,0a1,1 0,1 1,2 0a1,1 0,1 1,-2 0" />
            
    </group>
</vector>