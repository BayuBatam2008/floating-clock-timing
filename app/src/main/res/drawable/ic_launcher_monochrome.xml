<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Monochrome version for themed icons -->
    <group
        android:translateX="54"
        android:translateY="54">
        
        <!-- Main clock circle - outline only for monochrome -->
        <path
            android:fillColor="#000000"
            android:pathData="M0,0m-28,0a28,28 0,1 1,56 0a28,28 0,1 1,-56 0
                           M0,0m-24,0a24,24 0,1 0,48 0a24,24 0,1 0,-48 0" />
        
        <!-- Clock hands - showing 10:10 (classic time) -->
        <!-- Hour hand -->
        <path
            android:fillColor="#000000"
            android:pathData="M-1.5,-2h3v-12h-3z" />
        
        <!-- Minute hand -->
        <path
            android:fillColor="#000000"
            android:pathData="M-1,-2h2v-16h-2z" />
        
        <!-- Center dot -->
        <path
            android:fillColor="#000000"
            android:pathData="M0,0m-2.5,0a2.5,2.5 0,1 1,5 0a2.5,2.5 0,1 1,-5 0" />
        
        <!-- Single floating element for "floating" concept -->
        <path
            android:fillColor="#000000"
            android:pathData="M20,-12m-4,0a4,4 0,1 1,8 0a4,4 0,1 1,-8 0" />
            
        <!-- Hour markers (12, 3, 6, 9) -->
        <path
            android:fillColor="#000000"
            android:pathData="M-1,-22h2v-4h-2z" />  <!-- 12 o'clock -->
        <path
            android:fillColor="#000000"
            android:pathData="M22,-1h4v2h-4z" />   <!-- 3 o'clock -->
        <path
            android:fillColor="#000000"
            android:pathData="M-1,22h2v4h-2z" />   <!-- 6 o'clock -->
        <path
            android:fillColor="#000000"
            android:pathData="M-26,-1h4v2h-4z" />  <!-- 9 o'clock -->
            
    </group>
</vector>